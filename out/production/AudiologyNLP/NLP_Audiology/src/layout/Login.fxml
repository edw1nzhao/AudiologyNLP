<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="NLP_Audiology.src.controller.Login"
      prefHeight="400.0" prefWidth="600.0">

    <children>
        <TextField fx:id="ID" layoutX="103.0" layoutY="136.0" prefHeight="34.0" prefWidth="412.0"/>
        <PasswordField fx:id="password" layoutX="103.0" layoutY="225.0" prefHeight="34.0" prefWidth="412.0"/>
        <Label layoutX="103.0" layoutY="106.0" prefHeight="14.0" prefWidth="91.0" text="User ID">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label layoutX="103.0" layoutY="189.0" text="Password">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Button layoutX="450.0" layoutY="302.0" mnemonicParsing="false" onAction="#loginButtonAction" prefHeight="34.0"
                prefWidth="74.0" text="Login"/>
        <Button layoutX="358.0" layoutY="302.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="74.0" text="Exit"/>
    </children>
</Pane>
